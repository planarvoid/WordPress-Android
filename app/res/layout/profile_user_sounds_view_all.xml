<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        style="@style/Profile.Sounds.ViewAll.Container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        tools:ignore="UselessParent">

        <com.soundcloud.android.view.CustomFontTextView
            android:id="@+id/sounds_view_all_text"
            style="@style/Profile.Sounds.ViewAll.Text"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:drawableRight="@drawable/chevron" />
    </LinearLayout>
</FrameLayout>
