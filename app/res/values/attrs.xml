<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PlayerTimeView">
        <attr name="show_arrow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="JaggedTextView">
        <attr name="jagged_background" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="CustomFontTextView">
        <attr name="custom_font" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomFontTitleToolbar">
        <attr name="darkMode" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AspectRatioView">
        <attr name="ariv_aspectRatio" format="float" />
        <attr name="ariv_aspectRatioEnabled" format="boolean" />
        <attr name="ariv_dominantMeasurement">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
        <attr name="foreground" format="reference" />
    </declare-styleable>

    <declare-styleable name="SquareFrameLayout">
        <attr name="sfl_dominantMeasurement">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ParallaxImageView">
        <attr name="movement" format="dimension" />
        <attr name="focalPoint" format="float" />
    </declare-styleable>

    <declare-styleable name="OptimisedImageView">
        <attr name="showGradient" format="boolean" />
        <attr name="gradientStart" format="float" />
        <attr name="gradientStartColor" format="reference|color" />
        <attr name="gradientEndColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="WaveformView">
        <attr name="progressAboveStart" format="color" />
        <attr name="progressAboveEnd" format="color" />
        <attr name="progressBelow" format="color" />
        <attr name="unplayedAbove" format="color" />
        <attr name="unplayedBelow" format="color" />
        <attr name="widthRatio" format="float" />
        <attr name="barWidth" format="dimension" />
        <attr name="spaceWidth" format="dimension" />
        <attr name="baseline" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CollectionPreviewView">
        <attr name="collectionTitleEnabled" format="boolean" />
        <attr name="collectionTitle" format="string" />
        <attr name="collectionIcon" format="integer" />
        <attr name="collectionPreviewOverlay" format="integer" />
    </declare-styleable>

    <declare-styleable name="DownloadImageView">
        <attr name="queued" format="integer" />
        <attr name="downloaded" format="integer" />
        <attr name="downloading" format="integer" />
        <attr name="unavailable" format="integer" />
    </declare-styleable>

    <declare-styleable name="RoundedColorButton">
        <attr name="backgroundColorStateList" format="reference" />
    </declare-styleable>

    <declare-styleable name="CircularBorderImageView">
        <attr name="cbiv_borderWidth" format="dimension" />
        <attr name="cbiv_borderColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="LoadingButton">
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="loadingText" format="string" />
        <attr name="actionText" format="string" />
        <attr name="retryText" format="string" />
        <attr name="loadingColor" format="color" />
    </declare-styleable>

    <declare-styleable name="IconToggleButton">
        <attr name="checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ProfileToggleButton">
        <attr name="textOn" format="string" />
        <attr name="textOff" format="string" />
        <attr name="textPaddingRight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ForegroundImageView">
        <attr name="square" format="boolean" />
        <attr name="foregroundDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="PlayQueueSwipeRemoveItem">
        <attr name="android:textSize" />
        <attr name="android:text" />
        <attr name="android:background" />
        <attr name="android:textColor" />
        <attr name="android:paddingRight" />
        <attr name="custom_font" />
    </declare-styleable>

    <declare-styleable name="AutoResizeTextView">
        <attr name="minTextSize" format="dimension" />
        <attr name="resizeStep" format="dimension" />
        <attr name="addEllipsis" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GlassLinearLayout">
        <attr name="source" format="reference" />
        <attr name="blurRadius" format="float" />
    </declare-styleable>


</resources>
