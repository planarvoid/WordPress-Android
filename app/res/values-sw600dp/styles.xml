    <style name="BigProfileImage">
        <item name="android:background">@drawable/placeholder_cells</item>
        <item name="android:scaleType">centerCrop</item>
        <item name="showGradient">true</item>
        <item name="gradientStart">.7</item>
        <item name="gradientStartColor">@color/image_gradient_start</item>
        <item name="gradientEndColor">@color/image_gradient_end_profile</item>
    </style>
