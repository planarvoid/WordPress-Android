<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/ad_inlay_cta_border_radius" />
            <stroke android:color="@color/ak_dark_gray"
                android:width="1dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/ad_inlay_cta_border_radius" />
            <stroke android:color="@color/ak_promoted_teal_dark"
                android:width="1dp" />
        </shape>
    </item>
</selector>
